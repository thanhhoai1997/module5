<h1><%= form.label %></h1>
<form name="<%= form.name %>" action="<%= form.action %>" method="<%= form.method %>">
    <fieldset name="<%= model.name %>">
        <% Object.keys(model.attributes).forEach( function (attrKey) {
            <%- include('attribute.ejs', {attribute: model.attributes[attrKey]}) %>
        <% })%>
    </fieldset>
</form>